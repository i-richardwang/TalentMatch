# Langfuse API Keys (for logging and monitoring)
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_HOST=http://localhost:3000

# Tavily API Key (for web search)
TAVILY_API_KEY=your_tavily_api_key_here

# Embedding API Configuration (for text embedding)
EMBEDDING_API_KEY=your_embedding_api_key_here
EMBEDDING_API_BASE=https://api.example.com/v1/embeddings
EMBEDDING_MODEL=BAAI/bge-m3

# Vector Database Configuration
# Option 1: Self-hosted Milvus (use these)
VECTOR_DB_HOST=localhost
VECTOR_DB_PORT=19530
VECTOR_DB_DATABASE=examples
VECTOR_DB_DATABASE_RESUME=resume

# Option 2: Zilliz Cloud (use these instead, do not set db_name related vars)
# VECTOR_DB_URI=https://your-cluster.serverless.region.cloud.zilliz.com
# VECTOR_DB_TOKEN=your_api_token_here

# MySQL Database Configuration
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password_here
MYSQL_DATABASE=talentmatch

# OpenAI Compatible API Configuration
# Supports any OpenAI-compatible service (DeepSeek, SiliconCloud, OpenAI, etc.)
OPENAI_API_KEY=your_api_key_here
OPENAI_API_BASE=https://api.deepseek.com/v1

# LLM Model Configuration
LLM_MODEL=deepseek-chat

# Smart LLM Configuration (for advanced reasoning tasks)
SMART_LLM_MODEL=deepseek-chat

# MinIO Object Storage Configuration
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET_NAME=resumes